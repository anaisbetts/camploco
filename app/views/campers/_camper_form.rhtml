<% form_for_resource do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Rank</b><br />
    <%= f.text_field :rank %>
  </p>

  <p>
    <b>Age</b><br />
    <%= f.text_field :age %>
  </p>

  <p>
    <b>Participating in Nicoteh:</b>
    <%= f.check_box :nicoteh %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
